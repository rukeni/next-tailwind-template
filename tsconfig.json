{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": false,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "paths": {
      "@/styles/global.css": [
        "src/styles/global.css"
      ],
      "@global.css": [
        "src/styles/global.css"
      ]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES5"
  },
  "exclude": [
    "storybook-static",
  ],
  "include": [
    "next-env.d.ts",
    "src/**/*"
  ]
}
